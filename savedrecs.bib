@article{ WOS:000716425700007,
Author = {Kanglang, Liu and Afzaal, Muhammad},
Title = {Artificial Intelligence (AI) and Translation Teaching: A Critical
   Perspective on the Transformation of Education},
Journal = {INTERNATIONAL JOURNAL OF EDUCATIONAL SCIENCES},
Year = {2021},
Volume = {33},
Number = {1-3},
Pages = {64-73},
Month = {APR-JUN},
Abstract = {The majority of the universities and private institutions have initiated
   the use of artificial intelligence (AI) and machine translation (MT) in
   teaching translation. Translators have been trained by a systematic
   teaching method with newly designed curriculum with the addition of
   computer-assisted technology. However, the learner's face-to-face
   experience is relating them to advance self-learning of languages
   through AI machine, which lack the motivational mechanism. This review
   paper presents the recent advancement in the use of AI and MT in the
   teaching translations to translators. The aim of the study is to
   investigate the pedagogical implications of AI for teaching translation
   studies. The study concludes that there is lack of critical reflection
   of challenges and jeopardies of AI in translation teaching, there is a
   weak connection to academic instructive perceptions, and that there is a
   need for further exploration of principled and enlightening approaches
   in the application of AI in translation teaching in higher education.},
